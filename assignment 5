name = 'Alice'
print(name == 'Alice') 
print(name != 'Bob')  


print(name.lower() == 'alice') 
print(name.lower() == 'ALICE')  


age = 25
print(age == 25)  
print(age != 30) 
print(age > 20)  
print(age < 30) 
print(age >= 25)  
print(age <= 24)  

print(age > 20 and age < 30) 
print(age < 20 or age > 30)  


colors = ['red', 'green', 'blue']
print('green' in colors)
print('yellow' not in colors) 
print('blue' not in colors) 
